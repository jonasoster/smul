<?xml version="1.0" encoding="utf-8"?>
 <diagram>
   <elements>
     <obj label="o1">object1: Class</obj>
     <obj label="o2">object2: AnotherClass</obj>
     <obj label="o3">object3: DifferentClass</obj>
   </elements>

   <sequence>
     <activate label="o1"/>
     <step/>

     <arrow type="call" from="o1" to="o2">foo()</arrow>
     <activate label="o2"/>
     <step/>

     <arrow type="return" from="o2" to="o1"/>
     <deactivate label="o2"/>
     <step/>

     <arrow type="call" from="o1" to="o3">bar()</arrow>
     <activate label="o3"/>
     <step/>

     <arrow type="call" from="o3" to="o3">meep()</arrow>
     <step/>

     <arrow type="call" from="o3" to="o3">baz()</arrow>
     <activate label="o3"/>
     <step/>
     
     <deactivate label="o3"/>
     <step/>

     <arrow type="return" from="o3" to="o1">quux</arrow>
     <deactivate label="o3"/>
     <step amount="10"/>
     
     <deactivate label="o1"/>
     <step/>
   </sequence>
 </diagram>
